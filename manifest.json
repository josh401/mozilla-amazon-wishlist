{

  "description": "Displays a notice including wish list number of items and wish list subtotal.",
  "manifest_version": 2,
  "name": "Amazon Wishlist Subtotal",
  "version": "1.0",


  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.amazon.com/gp/registry/wishlist/*"],
      "js": ["js/jquery.js", "js/tooltipster.bundle.min.js", "js/wishlist_subtotal.js"],
	  "css": ["css/tooltipster.bundle.min.css", "css/style.css"]
    }
  ]
  
}